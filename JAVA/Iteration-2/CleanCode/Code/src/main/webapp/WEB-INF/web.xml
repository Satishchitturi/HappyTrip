    <url-pattern>/admin/addAirlineConfirm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddFlightConfirm</servlet-name>
    <servlet-class>com.happytrip.servlets.AddFlightConfirm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddFlightConfirm</servlet-name>
    <url-pattern>/admin/addFlightConfirm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddRouteConfirm</servlet-name>
    <servlet-class>com.happytrip.servlets.AddRouteConfirm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddRouteConfirm</servlet-name>
    <url-pattern>/admin/addRouteConfirm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddScheduleConfirm</servlet-name>
    <servlet-class>com.happytrip.servlets.AddScheduleConfirm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddScheduleConfirm</servlet-name>
    <url-pattern>/admin/addScheduleConfirm</url-pattern>
  </servlet-mapping>
