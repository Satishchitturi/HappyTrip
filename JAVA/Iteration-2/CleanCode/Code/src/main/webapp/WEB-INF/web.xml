    <servlet-name>ProfileController</servlet-name>
    <url-pattern>/profile.html</url-pattern>
    <url-pattern>/mytrips.html</url-pattern>
    <url-pattern>/viewTrip</url-pattern>
    <url-pattern>/saveProfile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.happytrip.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
