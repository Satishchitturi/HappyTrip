<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>classpath:spring/*-context.xml</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <context-param>
    <param-name>roomname</param-name>
    <param-value>Pratian</param-value>
  </context-param>
  <listener>
    <listener-class>com.happytrip.listeners.SessionListener</listener-class>
  </listener>
  <filter>
    <filter-name>genericfilter</filter-name>
    <filter-class>com.happytrip.filters.GenericFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>genericfilter</filter-name>
    <servlet-name>LoginServlet</servlet-name>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.happytrip.servlets.ChatloginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/chat/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ChatLogout</servlet-name>
    <servlet-class>com.happytrip.servlets.ChatlogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChatLogout</servlet-name>
    <url-pattern>/chat/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MessageManServlet</display-name>
    <servlet-name>MessageManServlet</servlet-name>
    <servlet-class>com.happytrip.servlets.ChatmessengerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MessageManServlet</servlet-name>
    <url-pattern>/chat/MessageManServlet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <filter>
    <filter-name>AdminFilter</filter-name>
    <filter-class>com.happytrip.filters.AdminFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AdminFilter</filter-name>
    <url-pattern>/admin/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>sessionFactoryUtil</servlet-name>
    <servlet-class>com.happytrip.util.SessionFactoryUtil</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>startupController</servlet-name>
    <servlet-class>com.happytrip.controllers.StartupController</servlet-class>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>FlightServlet</servlet-name>
    <servlet-class>com.happytrip.servlets.FlightServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FlightServlet</servlet-name>
    <url-pattern>/admin/addFlight</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminController</servlet-name>
    <servlet-class>com.happytrip.controllers.AdminController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminController</servlet-name>
    <url-pattern>/admin/addAirline</url-pattern>
    <url-pattern>/admin/addRoute</url-pattern>
    <url-pattern>/admin/scheduleFlight</url-pattern>
    <url-pattern>/admin/addFlight.html</url-pattern>
    <url-pattern>/admin/addAirline.html</url-pattern>
    <url-pattern>/admin/addRoute.html</url-pattern>
    <url-pattern>/admin/scheduleFlight.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ReportController</servlet-name>
    <servlet-class>com.happytrip.controllers.ReportController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReportController</servlet-name>
    <url-pattern>/admin/reports.html</url-pattern>
    <url-pattern>/admin/report/listAllBookings.html</url-pattern>
    <url-pattern>/admin/report/listAllUsers.html</url-pattern>
    <url-pattern>/admin/report/listAllAirline.html</url-pattern>
    <url-pattern>/admin/report/listAllScheduledFlight.html</url-pattern>
    <url-pattern>/admin/report/listAllRoutes.html</url-pattern>
  </servlet-mapping>
