    <servlet-name>RegisterConfirm</servlet-name>
    <servlet-class>com.happytrip.servlets.RegisterConfirm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterConfirm</servlet-name>
    <url-pattern>/registerConfirm</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>FlightValidator</display-name>
    <servlet-name>FlightValidator</servlet-name>
    <servlet-class>com.happytrip.servlets.FlightValidator</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FlightValidator</servlet-name>
    <url-pattern>/admin/FlightValidator</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ScheduleValidator</display-name>
    <servlet-name>ScheduleValidator</servlet-name>
    <servlet-class>com.happytrip.servlets.ScheduleValidator</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScheduleValidator</servlet-name>
    <url-pattern>/admin/ScheduleValidator</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProfileValidator</display-name>
    <servlet-name>ProfileValidator</servlet-name>
    <servlet-class>com.happytrip.servlets.ProfileValidator</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProfileValidator</servlet-name>
