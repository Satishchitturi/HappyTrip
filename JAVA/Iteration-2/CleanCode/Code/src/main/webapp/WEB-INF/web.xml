    <servlet-name>HomeController</servlet-name>
    <url-pattern>/cities</url-pattern>
    <url-pattern>/home.html</url-pattern>
    <url-pattern>/getAllCities</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Authentication</servlet-name>
    <servlet-class>com.happytrip.servlets.AuthenticationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Authentication</servlet-name>
    <url-pattern>/authenticate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ProfileController</servlet-name>
    <servlet-class>com.happytrip.controllers.ProfileController</servlet-class>
  </servlet>
