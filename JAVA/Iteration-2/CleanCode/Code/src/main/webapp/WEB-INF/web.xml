    <servlet-name>FlightPaymentController</servlet-name>
    <servlet-class>com.happytrip.controllers.flight.FlightPaymentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FlightPaymentController</servlet-name>
    <url-pattern>/flight/confirmBooking.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PaymentProcessController</servlet-name>
    <servlet-class>com.happytrip.controllers.flight.PaymentProcessController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PaymentProcessController</servlet-name>
    <url-pattern>/paymentConfirmation</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ContentController</servlet-name>
    <servlet-class>com.happytrip.controllers.ContentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContentController</servlet-name>
    <url-pattern>/aboutus</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegisterController</servlet-name>
    <servlet-class>com.happytrip.controllers.RegisterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterController</servlet-name>
    <url-pattern>/register.html</url-pattern>
    <url-pattern>/registerprocess</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>com.happytrip.controllers.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/login.html</url-pattern>
    <url-pattern>/loginAdmin.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PassengerController</servlet-name>
    <servlet-class>com.happytrip.controllers.PassengerController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PassengerController</servlet-name>
    <url-pattern>/passengerFlights.html</url-pattern>
  </servlet-mapping>
